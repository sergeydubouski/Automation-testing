<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="AlertsPageTestSuite" verbose="1" parallel="tests"
	thread-count="2">
	<test name="AlertsPageTestSuite-CH">
		<parameter name="browser" value="chrome" />
		<parameter name="message" value="Test+*/-\@" />
		<classes>
			<class name="com.heroku.theinternet.alerts.AlertsPageTest" />
		</classes>
	</test>
	<test name="AlertsPageTestSuite-FF">
		<parameter name="browser" value="firefox" />
		<parameter name="message" value="100000000\s%" />
		<classes>
			<class name="com.heroku.theinternet.alerts.AlertsPageTest" />
		</classes>
	</test>
</suite>